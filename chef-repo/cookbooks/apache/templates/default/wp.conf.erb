<VirtualHost *:80>
  ServerName centos-webdb
  DocumentRoot "<%= @webroot %>"

  # Settings for logging
  LogFormat "%a %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-Agent}i\" %D" aws_elb
  ErrorLog logs/error_log

  <Directory <%= @webroot %>>
    Options FollowSymLinks
    AllowOverride All
    Require all granted
  </Directory>
</VirtualHost>
